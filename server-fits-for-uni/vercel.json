{
    "version": 2,
    "name": "Fits For Uni",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/*": {
            "memory": 256, 
            "maxDuration": 10 
        }
    }
}
